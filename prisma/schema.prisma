// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model member {
  id          Int      @id @default(autoincrement())
  name        String
  birthday    String
}

model examinees {
  no                String
  type              String
  school            String
  department        String
  major             String
  id                String        @id
  name              String
  ns_type           String
  special           String
  birthday          String
  gender            String
  ns_year           String
  ns_code           String
  ns_name           String
  ns_department     String
  phone             String
  cellphone         String
  email             String
  zip               String
  address1          String
  address2          String
  nation            String
  pa_phone          String
  pa_cellphone      String
  pa_email          String
  pa_name           String
  soldier           String
  agree_flag        String
  grad_date         String
  grad_type         String
  pa_relation       String
  receipted         String
  hanja             String
  memo              String
  etc               String
  except_flag       String
  print             String
  created           DateTime @default(now())
  updated           DateTime @updatedAt
}